<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>POSOrderServlet</servlet-name>
        <servlet-class>com.si.servlet.POSOrderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>POSMenuServlet</servlet-name>
        <servlet-class>com.si.servlet.POSMenuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderProcessServlet</servlet-name>
        <servlet-class>com.si.servlet.OrderProcessServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderIDServlet</servlet-name>
        <servlet-class>com.si.servlet.OrderIDServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>POSOrderServlet</servlet-name>
        <url-pattern>/POSOrderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>POSMenuServlet</servlet-name>
        <url-pattern>/POSMenuServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderProcessServlet</servlet-name>
        <url-pattern>/OrderProcessServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderIDServlet</servlet-name>
        <url-pattern>/OrderIDServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>sis-menu-online/index.html</welcome-file>
    </welcome-file-list>
</web-app>
